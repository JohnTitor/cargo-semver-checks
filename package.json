{
  "name": "semver-labeler-action",
  "version": "0.2.1",
  "private": true,
  "sideEffects": [
    "./src/stdio.ts"
  ],
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/index.ts -o dist",
