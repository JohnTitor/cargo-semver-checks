    "@actions/github": "^7.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vercel/ncc": "^0.38.4",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.26.2"
}
